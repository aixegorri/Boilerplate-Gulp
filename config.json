{
	"fonts": {
		"src": "_source/assets/fonts/**/**.*",
		"dest": "dist/assets/fonts"
	},
	"images": {
		"src": "_source/assets/img/**/*.{gif,jpg,jpeg,png,svg}",
		"dest": "dist/assets/img"
	},
	"vendorJS": {
		"src": "_source/assets/js/vendor/*.js",
		"dest": "dist/assets/js/vendor"
	},
	"statics": {
		"src": "_source/statics/**.*",
		"dest": "dist"
	},
	"docs": {
		"src": [
			"_source/docs/**/*.html"
		],
		"dest": "dist"
	},
	"styles": {
		"src": "_source/assets/scss/**/*.s*ss",
		"dest": "dist/assets/css"
	},
	"plugins": {
		"src": [
			"_source/assets/js/plugins/_plugins.js",
			"_source/assets/js/plugins/jquery.cookie.js"
		],
		"dest": "dist/assets/js/"
	},
	"scripts": {
		"src": [
			"_source/assets/js/_sn.js",
			"_source/assets/js/_sn.cache.js",
			"_source/assets/js/main.js"
		],
		"dest": "dist/assets/js/"
	},
	"watch": {
		"images": [
			"_source/assets/img/**/*.{gif,jpg,jpeg,png,svg}"
		],
		"vendorJS": [
			"_source/assets/js/vendor/*.js"
		],
		"statics": [
			"_source/statics/**.*"
		],
		"scripts": [
			"_source/assets/js/**/*.js"
		],
		"styles": [
			"_source/assets/scss/**/*.{scss,sass,css}"
		],
		"docs": [
			"_source/docs/**/*.html"
		],
		"html": [
			"dist/**/*.html",
			"!dist/assets"
		]
	}
}
